{
  "name": "prisma-extension-random",
  "version": "0.1.2",
  "author": "Nicolas Keil <nkeil.dev@gmail.com>",
  "description": "Add some randomness to your favorite Prisma queries!",
  "repository": {
    "type": "git",
    "url": "https://github.com/nkeil/prisma-extension-random"
  },
  "keywords": [
    "prisma",
    "@prisma/client",
    "extension",
    "random"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "/dist"
  ],
  "scripts": {
    "build": "rimraf dist && tsc",
    "format": "prettier --write src/**/*.ts *.md --ignore-unknown --no-error-on-unmatched-pattern",
    "type-check": "tsc --noEmit"
  },
  "peerDependencies": {
    "@prisma/client": "latest"
  },
  "devDependencies": {
    "@prisma/client": "latest",
    "prettier": "^3.0.3",
    "prisma": "latest",
    "rimraf": "^5.0.5",
    "typescript": "4.9.4"
  }
}
